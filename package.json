{
	"name": "madcampos-site",
	"private": true,
	"version": "3.5.0",
	"license": "MIT",
	"type": "module",
	"engines": { "node": ">=22.0.0" },
	"volta": { "node": "22.11.0" },
	"scripts": {
		"prepare": "husky",
		"postinstall": "astro sync",
		"start": "astro dev --site=http://localhost:3000/",
		"build": "astro check && astro build",
		"prepreview": "npm run build",
		"preview": "astro preview --site=http://localhost:3000/",
		"typecheck": "tsc --noEmit",
		"format": "dprint fmt --staged",
		"lint:js": "eslint --fix",
		"lint:md": "astro check",
		"lint": "npm run typecheck && npm run lint:js && npm run lint:md"
	},
	"devDependencies": {
		"@astrojs/check": "^0.9.4",
		"@astrojs/rss": "^4.0.10",
		"@astrojs/sitemap": "^3.2.1",
		"@iconify-json/brandico": "^1.2.2",
		"@iconify-json/fa6-brands": "^1.2.5",
		"@iconify-json/uil": "^1.2.3",
		"@shikijs/transformers": "^1.24.2",
		"@shikijs/twoslash": "^1.24.2",
		"@types/hast": "^3.0.4",
		"@types/mdast": "^4.0.4",
		"@types/node": "^22.10.2",
		"@typescript-eslint/eslint-plugin": "^8.18.1",
		"@typescript-eslint/parser": "^8.18.1",
		"@vite-pwa/astro": "^0.4.3",
		"astro": "^4.16.17",
		"astro-icon": "^1.1.4",
		"eslint": "^9.17.0",
		"eslint-plugin-astro": "^1.3.1",
		"globals": "^15.13.0",
		"husky": "^9.1.7",
		"mdast-util-directive": "^3.0.0",
		"rehype-external-links": "^3.0.0",
		"remark-behead": "^3.1.0",
		"remark-breaks": "^4.0.0",
		"remark-directive": "^3.0.0",
		"typescript": "^5.7.2",
		"unist-util-visit": "^5.0.0",
		"vfile": "^6.0.3"
	},
	"overrides": {
		"array-includes": "npm:@nolyfill/array-includes@^1",
		"array.prototype.flat": "npm:@nolyfill/array.prototype.flat@^1",
		"array.prototype.flatmap": "npm:@nolyfill/array.prototype.flatmap@^1",
		"es-iterator-helpers": "npm:@nolyfill/es-iterator-helpers@^1",
		"hasown": "npm:@nolyfill/hasown@^1",
		"is-core-module": "npm:@nolyfill/is-core-module@^1",
		"object.assign": "npm:@nolyfill/object.assign@^1",
		"object.entries": "npm:@nolyfill/object.entries@^1",
		"object.fromentries": "npm:@nolyfill/object.fromentries@^1",
		"object.values": "npm:@nolyfill/object.values@^1",
		"string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@^1",
		"yaml-language-server": { "prettier": ">=2.8.7" }
	},
	"dependencies": {
		"inapp-spy": "4.1.1"
	}
}
