{
	"name": "madcampos-site",
	"private": true,
	"version": "3.5.0",
	"license": "MIT",
	"type": "module",
	"engines": {
		"node": ">=22.0.0"
	},
	"volta": {
		"node": "22.18.0"
	},
	"scripts": {
		"start": "wrangler dev --https-cert-path certs/server.crt --https-key-path certs/server.key",
		"build": "node build/index.ts",
		"generate:types": "wrangler types env.d.ts",
		"deploy": "wrangler deploy --minify --env production",
		"typecheck": "tsc --noEmit",
		"format": "dprint fmt --staged",
		"lint:js": "eslint --fix",
		"lint:md": "astro check",
		"lint": "npm run typecheck && npm run lint:js && npm run lint:md"
	},
	"devDependencies": {
		"@jimp/core": "^1.6.0",
		"@jimp/wasm-jpeg": "^1.6.0",
		"@jimp/wasm-png": "^1.6.0",
		"@jimp/wasm-webp": "^1.6.0",
		"@shikijs/transformers": "3.8.1",
		"@types/node": "^24.3.1",
		"@typescript-eslint/eslint-plugin": "^8.43.0",
		"@typescript-eslint/parser": "^8.43.0",
		"dprint": "0.50.1",
		"eslint": "^9.35.0",
		"globals": "^16.3.0",
		"htmlnano": "^2.1.4",
		"jimp": "^1.6.0",
		"lightningcss": "^1.30.1",
		"marked": "^16.3.0",
		"marked-footnote": "1.4.0",
		"marked-shiki": "1.2.1",
		"node-html-parser": "7.0.1",
		"shiki": "3.8.1",
		"typescript": "^5.9.2",
		"urlpattern-polyfill": "10.1.0",
		"web-features": "^2.48.0",
		"wrangler": "^4.35.0",
		"yaml": "^2.8.1"
	},
	"overrides": {},
	"dependencies": {
		"inapp-spy": "5.0.5"
	}
}
