---
import { Icon } from 'astro-icon/components';
---
<svg-defs aria-hidden="true">
	<!-- Table icons -->
	<Icon name="mingcute:line-height-line" id="table-sort-icon-none" aria-hidden="true" role="none" width="1" height="1" />
	<Icon name="mingcute:sort-ascending-line" id="table-sort-icon-descending" aria-hidden="true" role="none" width="1" height="1" />
	<Icon name="mingcute:sort-descending-line" id="table-sort-icon-ascending" aria-hidden="true" role="none" width="1" height="1" />

	<!-- Image lightbox icons -->
	<Icon name="mingcute:information-line" id="img-lightbox-icon-info" aria-hidden="true" role="none" width="1" height="1" />
	<Icon name="mingcute:external-link-line" id="img-lightbox-icon-open" aria-hidden="true" role="none" width="1" height="1" />
	<Icon name="mingcute:close-line" id="img-lightbox-icon-close" aria-hidden="true" role="none" width="1" height="1" />
	<Icon name="mingcute:refresh-anticlockwise-1-line" id="img-lightbox-icon-reset-zoom" aria-hidden="true" role="none" width="1" height="1" />
	<Icon name="mingcute:fullscreen-line" id="img-lightbox-icon-enter-fullscreen" aria-hidden="true" role="none" width="1" height="1" />
	<Icon name="mingcute:fullscreen-exit-line" id="img-lightbox-icon-exit-fullscreen" aria-hidden="true" role="none" width="1" height="1" />
	<Icon name="mingcute:download-2-line" id="img-lightbox-icon-download" aria-hidden="true" role="none" width="1" height="1" />

	<!-- Share options icons -->
	<Icon name="mingcute:share-forward-line" id="share-icon-share" aria-hidden="true" role="none" width="1" height="1" />
	<Icon name="mingcute:copy-line" id="share-icon-copy" aria-hidden="true" role="none" width="1" height="1" />
	<Icon name="mingcute:mail-send-line" id="share-icon-email" aria-hidden="true" role="none" width="1" height="1" />
	<Icon name="mingcute:message-4-line" id="share-icon-sms" aria-hidden="true" role="none" width="1" height="1" />
	<Icon name="mingcute:print-line" id="share-icon-print" aria-hidden="true" role="none" width="1" height="1" />

	<!-- Theme switcher icons -->
	<Icon name="mingcute:palette-3-line" id="theme-switcher-icon" aria-hidden="true" role="none" width="1" height="1" />
	<Icon name="mingcute:alert-line" id="theme-switcher-icon-warning" aria-hidden="true" role="none" width="1" height="1" />
	<svg viewBox="0 0 24 24" id="theme-switcher-icon-close" aria-hidden="true" role="none" width="1" height="1">
		<use href="#img-lightbox-icon-close" width="24" height="24" />
	</svg>

	<svg viewBox="0 0 10 10" aria-hidden="true" role="none" width="1" height="1">
		<defs>
			<filter id="small-pixelate-filter">
				<feGaussianBlur result="blurred-image" stdDeviation="1" />
				<feImage
					href="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAUAAAAFCAIAAAACDbGyAAAAAXNSR0IArs4c6QAAAARnQU1BAACxjwv8YQUAAAAJcEhZcwAADsMAAA7DAcdvqGQAAAAWSURBVAgdY1ywgOEDAwKxgJhIgFQ+AP/vCNK2s+8LAAAAAElFTkSuQmCC"
					width="3"
					height="3"
				/>
				<feTile result="displacement-map" />
				<feDisplacementMap
					in="blurred-image"
					in2="displacement-map"
					result="pixelated-image"
					scale="8"
					xChannelSelector="R"
					yChannelSelector="G"
				/>
			</filter>
			<filter id="glitch-filter">
				<feTurbulence baseFrequency="0 1.01" numOctaves="32" seed="2" />
				<feMorphology operator="dilate" radius="8" result="glitch-lines" />
				<feDisplacementMap in="SourceGraphic" in2="glitch-lines" scale="32" xChannelSelector="R" yChannelSelector="R" />
				<feOffset dy="-2" />
			</filter>
			<filter id="animated-glitch-filter">
				<feTurbulence baseFrequency="0 2" numOctaves="32">
					<animate attributeName="numOctaves" dur="16s" repeatCount="indefinite" values="8;2;16;2;8;32;8;2;8;16;32;4;2;8;16;64" />
					<animate attributeName="baseFrequency" dur="30s" repeatCount="indefinite" values="0 0.001; 0 0.002" />
					<animate
						additive="sum"
						attributeName="baseFrequency"
						calcMode="discrete"
						dur="60s"
						repeatCount="indefinite"
						values="0 0.001;0 2;0 3;0 0.001;0 0.001;0.001 5;0 2;0 3;0 2;0 0.001;0.5 2;1 0.05;0 0.001;0 0.001;0 4;0 0.001;0.001 3;0 0.1;0 0.001;0 2;0 6;0 2;0 0.001;0.001 1;0 0.001;0 0.001;0 2;0 0.001;"
					/>
					<animate attributeName="seed" calcMode="discrete" dur="120s" repeatCount="indefinite" values="1;2;3;4;5" />
				</feTurbulence>
				<feMorphology operator="dilate" radius="8" result="glitch-lines" />
				<feDisplacementMap in="SourceGraphic" in2="glitch-lines" scale="32" xChannelSelector="R" yChannelSelector="R" />
			</filter>
			<filter id="pixelate-filter">
				<feGaussianBlur result="blurred-image" stdDeviation="8" />
				<feImage
					href="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAUAAAAFCAIAAAACDbGyAAAAAXNSR0IArs4c6QAAAARnQU1BAACxjwv8YQUAAAAJcEhZcwAADsMAAA7DAcdvqGQAAAAWSURBVAgdY1ywgOEDAwKxgJhIgFQ+AP/vCNK2s+8LAAAAAElFTkSuQmCC"
					width="15"
					height="15"
				/>
				<feTile result="displacement-map" />
				<feDisplacementMap
					in="blurred-image"
					in2="displacement-map"
					result="pixelated-image"
					scale="50"
					xChannelSelector="R"
					yChannelSelector="G"
				/>
				<feOffset dx="-12" dy="-12" />
			</filter>
			<filter id="noise-filter">
				<feGaussianBlur result="blurred-image" stdDeviation="16" />
				<feTurbulence baseFrequency="1.2" numOctaves="10" stitchTiles="stitch" type="fractalNoise" />
				<feColorMatrix type="saturate" values="0" />
				<feComposite in2="blurred-image" operator="in" />
				<feComponentTransfer>
					<feFuncA slope=".7" type="linear" />
				</feComponentTransfer>
			</filter>
		</defs>
	</svg>
	<svg viewBox="0 0 24 24" aria-hidden="true" role="none" width="1" height="1">
		<symbol id="mingcute--external-link-line" viewBox="0 0 24 24">
			<g fill="none">
				<path d="m12.593 23.258l-.011.002l-.071.035l-.02.004l-.014-.004l-.071-.035q-.016-.005-.024.005l-.004.01l-.017.428l.005.02l.01.013l.104.074l.015.004l.012-.004l.104-.074l.012-.016l.004-.017l-.017-.427q-.004-.016-.017-.018m.265-.113l-.013.002l-.185.093l-.01.01l-.003.011l.018.43l.005.012l.008.007l.201.093q.019.005.029-.008l.004-.014l-.034-.614q-.005-.018-.02-.022m-.715.002a.02.02 0 0 0-.027.006l-.006.014l-.034.614q.001.018.017.024l.015-.002l.201-.093l.01-.008l.004-.011l.017-.43l-.003-.012l-.01-.01z" />
				<path
					fill="currentColor"
					d="M11 6a1 1 0 1 1 0 2H5v11h11v-6a1 1 0 1 1 2 0v6a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V8a2 2 0 0 1 2-2zm9-3a1 1 0 0 1 1 1v5a1 1 0 1 1-2 0V6.414l-8.293 8.293a1 1 0 0 1-1.414-1.414L17.586 5H15a1 1 0 1 1 0-2Z"
				/>
			</g>
		</symbol>
		<symbol id="mingcute--arrow-to-up-line" viewBox="0 0 24 24">
			<g fill="none">
				<path d="M24 0v24H0V0zM12.594 23.258l-.012.002l-.071.035l-.02.004l-.014-.004l-.071-.036q-.016-.004-.024.006l-.004.01l-.017.428l.005.02l.01.013l.104.074l.015.004l.012-.004l.104-.074l.012-.016l.004-.017l-.017-.427q-.004-.016-.016-.018m.264-.113l-.014.002l-.184.093l-.01.01l-.003.011l.018.43l.005.012l.008.008l.201.092q.019.005.029-.008l.004-.014l-.034-.614q-.005-.018-.02-.022m-.715.002a.02.02 0 0 0-.027.006l-.006.014l-.034.614q.001.018.017.024l.015-.002l.201-.093l.01-.008l.003-.011l.018-.43l-.003-.012l-.01-.01z" />
				<path
					fill="currentColor"
					d="M19 5a1 1 0 1 0 0-2H5a1 1 0 1 0 0 2zM7.05 12.703a1 1 0 0 0 1.415 0L11 10.167V20a1 1 0 0 0 2 0v-9.833l2.536 2.536a1 1 0 0 0 1.414-1.415l-4.243-4.242a1 1 0 0 0-1.414 0L7.05 11.288a1 1 0 0 0 0 1.415"
				/>
			</g>
		</symbol>
	</svg>
</svg-defs>
