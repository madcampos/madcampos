---
import './styles.css';

interface Props {
	subtitle: string;
	shortSubtitle: string;
	url?: URL | string;
	urlTitle?: string;
}

function generateUniqueId() {
	return Math.trunc(Math.random() * 1000000).toString(16);
}

function generateSubtitle(paddingStart: number, paddingEnd: number, subtitle: string) {
	return [
		...new Array(paddingStart).fill(0).map(() => `<tspan class="logo-text-padding-end">-</tspan>`),
		...[...subtitle].map((char) => `<tspan class="logo-text-content">${char}</tspan>`),
		...new Array(paddingEnd).fill(0).map(() => `<tspan class="logo-text-padding-end">-</tspan>`)
	].join('');
}

const { url, urlTitle, subtitle, shortSubtitle } = Astro.props;

const logoLabelId = generateUniqueId();
