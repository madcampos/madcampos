<a href={url} class="u-url url">
	<svg
		xmlns="http://www.w3.org/2000/svg"
		xmlns:xlink="http://www.w3.org/1999/xlink"
		color-interpolation-filters="sRGB"
		preserveAspectRatio="xMinYMin slice"
		id="logo-graphics"
		class="u-logo"
		width="100%"
		height="100%"
		role="img"
		aria-labelledby={`logo-label-${logoLabelId}`}
		shape-rendering="geometricPrecision"
	>
		<title id={`logo-label-${logoLabelId}`}>Marco Campos' Logo{url ? `, to ${urlTitle ?? ''}` : ''}</title>

		<!-- Effects -->
		<defs>
			<linearGradient id="sunset" x1="0" x2="0" y1="0" y2="100%">
				<stop offset="5%" stop-color="royalblue" />
				<stop offset="45%" stop-color="lightskyblue" />
				<stop offset="50%" stop-color="white" />
				<stop offset="50%" stop-color="purple" />
				<stop offset="80%" stop-color="hotpink" />
				<stop offset="95%" stop-color="white" />
			</linearGradient>
		</defs>

		<!-- Text -->
		<defs>
			<text
				id="logo-text-micro"
				x="95%"
				y="85%"
				alignment-baseline="middle"
				dominant-baseline="middle"
				text-anchor="end"
				fill-rule="nonzero"
				text-rendering="optimizeLegibility"
			>
				<Fragment set:html={generateSubtitle(7, 2, shortSubtitle)} />
			</text>
			<text
				id="logo-text-mini"
				x="95%"
				y="85%"
