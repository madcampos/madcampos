---
import './styles.css';

type LogoSize = 'full' | 'mini' | 'micro';

interface Props {
	size: LogoSize;
	subtitle: string;
	shortSubtitle: string;
	url?: URL | string;
	urlTitle?: string;
}

const sizes = {
	full: {
		width: 3500,
		height: 900,
		subtitle: { length: 21, padEnd: 4 }
	},
	mini: {
		width: 2000,
		height: 900,
		subtitle: { length: 12, padEnd: 4 }
	},
	micro: {
		width: 1100,
		height: 900,
		subtitle: { length: 9, padEnd: 2 }
	}
} as const;

function formatSubttile(subtitle: string, shortSubtitle: string, size: LogoSize) {
	if (subtitle === '') {
		return '';
	}

	const { subtitle: { length, padEnd } } = sizes[size];
	const MIN_START_PADDING = 2;
	const MIN_END_PADDING = 2;

	let formattedSubtitle = subtitle;

	if (formattedSubtitle.length + MIN_END_PADDING + MIN_START_PADDING >= length) {
		formattedSubtitle = shortSubtitle;
	}

	if (formattedSubtitle.length + padEnd < length) {
		formattedSubtitle = `${formattedSubtitle}${'-'.repeat(padEnd)}`;
	} else {
		formattedSubtitle = `${formattedSubtitle}${'-'.repeat(MIN_END_PADDING)}`;
	}

	return formattedSubtitle.padStart(length, '-');
}

function generateUniqueId() {
	return Math.trunc(Math.random() * 1000000).toString(16);
}

const { size, url, urlTitle, subtitle, shortSubtitle } = Astro.props;
const { width, height } = sizes[size];

const logoLabelId = generateUniqueId();
