---
type LogoStyles = 'full' | 'mini' | 'micro';

interface Props {
	style: LogoStyles;
	title: string;
	subtitle: string;
	shortSubtitle: string;
	url?: URL | string;
}

const sizes = {
	full: {
		width: 3500,
		height: 900,
		subtitle: { length: 21, padEnd: 4 },
		altText: 'My moniker, "madcampos", on a monospaced font, in blue, between curly braces.'
	},
	mini: {
		width: 2000,
		height: 900,
		subtitle: { length: 12, padEnd: 4 },
		altText: 'The letters "madc" on a monospaced font, in blue, between curly braces.'
	},
	micro: {
		width: 1100,
		height: 900,
		subtitle: { length: 9, padEnd: 2 },
		altText: 'The letter "m" on a monospaced font, in blue, between curly braces.'
	}
} as const;

function formatSubttile(subtitle: string, shortSubtitle: string, style: LogoStyles) {
	if (subtitle === '') {
		return '';
	}

	const { subtitle: { length, padEnd } } = sizes[style];
	const MIN_START_PADDING = 2;
	const MIN_END_PADDING = 2;

	let formattedSubtitle = subtitle;

	if (formattedSubtitle.length + MIN_END_PADDING + MIN_START_PADDING >= length) {
		formattedSubtitle = shortSubtitle;
	}

	if (formattedSubtitle.length + padEnd < length) {
		formattedSubtitle = `${formattedSubtitle}${'-'.repeat(padEnd)}`;
	} else {
		formattedSubtitle = `${formattedSubtitle}${'-'.repeat(MIN_END_PADDING)}`;
	}

	return formattedSubtitle.padStart(length, '-');
}

function generateUniqueId() {
	return Math.trunc(Math.random() * 1000000).toString(16);
}

const { style, url, title, subtitle, shortSubtitle } = Astro.props;
const { width, height, altText: baseAltText } = sizes[style];

const altText = `${baseAltText} Below it, a subtext of "${subtitle}" in orange on the lower right corner.`;
const logoLabelId = generateUniqueId();
---
<a href={url} class="u-url url">
	<svg
		class="logo-graphics u-logo"
		width="100%"
		height="100%"
		viewBox={`0 0 ${width} ${height}`}
		role="img"
		aria-labeledby={`logo-label-${logoLabelId}`}
		aria-details={altText}
	>
		<title id={`logo-label-${logoLabelId}`}>{title}</title>
		<style>
			.logo-graphics {
				height: 100%;
				object-fit: contain;
				width: clamp(var(--min-width, 5rem), 70vmin, var(--medium-width, 30rem));
			}

			.logo-graphics text {
				fill: var(--accent-color, #ff8000);
				font-family: 'Mecano-Light', 'Mecano', monospace;
				font-size: 8rem;
				font-weight: 300;
				text-rendering: optimizeLegibility;
			}

			.logo-graphics g {
				fill: var(--theme-color, #0080ff);
				shape-rendering: geometricPrecision;
			}

			.logo-graphics [data-logo-style] { display: none; }
			.logo-graphics .logo-style-full [data-logo-style='full'] { display: block; }
			.logo-graphics .logo-style-mini [data-logo-style='mini'] { display: block; }
			.logo-graphics .logo-style-micro [data-logo-style='micro'] { display: block; }
		</style>
		<text
			alignment-baseline="middle"
			dominant-baseline="middle"
			text-anchor="end"
			x="95%"
			y="85%"
		>
			{formatSubttile(subtitle, shortSubtitle, style)}
		</text>
		<g class={`logo-style-${style}`}>
